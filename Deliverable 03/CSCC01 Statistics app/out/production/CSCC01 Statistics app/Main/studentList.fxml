<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/9" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Main.loginController">
   <children>
      <Label alignment="CENTER" layoutX="44.0" layoutY="23.0" prefHeight="23.0" prefWidth="256.0" text="Student List">
         <font>
            <Font name="System Bold" size="17.0" />
         </font></Label>
      <Button fx:id="addStudent" layoutX="384.0" layoutY="68.0" mnemonicParsing="false" onAction="#addStudent" prefHeight="54.0" prefWidth="167.0" text="Add Student" />
      <Button layoutX="384.0" layoutY="181.0" mnemonicParsing="false" prefHeight="54.0" prefWidth="167.0" text="Remove Student" />
      <Button fx:id="backButton" layoutX="384.0" layoutY="296.0" mnemonicParsing="false" onAction="#backMain" prefHeight="54.0" prefWidth="167.0" text="Back" />
      <TableView fx:id="studentTable" layoutX="28.0" layoutY="56.0" prefHeight="316.0" prefWidth="299.0">
        <columns>
          <TableColumn prefWidth="75.0" text="First Name" />
          <TableColumn prefWidth="104.0" text="Last Name" />
            <TableColumn prefWidth="117.0" text="Student ID" />
        </columns>
      </TableView>
   </children>
</AnchorPane>
