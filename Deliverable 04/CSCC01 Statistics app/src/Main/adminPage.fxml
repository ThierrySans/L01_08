<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/9" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Main.loginController">
   <children>
      <Label alignment="CENTER" contentDisplay="CENTER" layoutY="24.0" prefHeight="17.0" prefWidth="630.0" text="Welcome" textAlignment="CENTER">
         <font>
            <Font size="27.0" />
         </font>
      </Label>
      <Button fx:id="viewStudents" layoutX="65.0" layoutY="82.0" mnemonicParsing="false" onAction="#viewStudent" prefHeight="65.0" prefWidth="145.0" text="View Students" />
      <Button fx:id="viewAssignments" onAction="#viewAssignment" layoutX="65.0" layoutY="178.0" mnemonicParsing="false" prefHeight="65.0" prefWidth="145.0" text="View Assignments" />
      <Button fx:id="logOutButton" layoutX="65.0" layoutY="270.0" mnemonicParsing="false" onAction="#logOutHandler" prefHeight="65.0" prefWidth="145.0" text="Log Out" />
   </children>
</AnchorPane>
