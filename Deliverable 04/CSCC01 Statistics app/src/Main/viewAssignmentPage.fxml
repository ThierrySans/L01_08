<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.TextFlow?>
<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8.0.121" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Main.viewAssignmentController">
    <children>
        <Button fx:id="assgnBackToList" onAction="#aBackToList" prefHeight="30.0" prefWidth="70.0" text="Back"/>
        <VBox alignment="CENTER" layoutY="31">
            <Label fx:id="assgnTitle" alignment="CENTER" contentDisplay="CENTER" layoutY="24.0" prefHeight="17.0" prefWidth="630.0" textAlignment="CENTER">
                    <font>
                        <Font size="27.0" />
                    </font>
                </Label>
            <Button fx:id="loadAssignment" onAction="#loadAssignment" prefHeight="30" text="Load Assignment Questions" />
        </VBox>
        <TextFlow layoutX="50.0" layoutY="120.0">
            <VBox fx:id="assgnVbox">
                <children>
                    <Label text="hi" />
                    <Label text="hey" />
                </children>
            </VBox>
        </TextFlow>
    </children>
</AnchorPane>
