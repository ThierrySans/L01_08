<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox prefHeight="400.0" prefWidth="640.0" xmlns="http://javafx.com/javafx/8.0.121" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Main.loginController">
  <children>
    <AnchorPane maxHeight="-1.0" maxWidth="-1.0" prefHeight="367.0" prefWidth="657.0" VBox.vgrow="ALWAYS">
      <children>
            <TextField fx:id="userName" layoutX="277.0" layoutY="127.0" />
            <PasswordField fx:id="password" layoutX="277.0" layoutY="178.0" />
            <Label layoutX="177.0" layoutY="132.0" prefHeight="17.0" prefWidth="87.0" text="User Name: " />
            <Label layoutX="177.0" layoutY="182.0" prefHeight="17.0" prefWidth="87.0" text="Password: " />
            <Label alignment="CENTER" layoutX="119.0" layoutY="42.0" prefHeight="55.0" prefWidth="381.0" text="Login" textAlignment="CENTER">
               <font>
                  <Font name="Arial" size="36.0" />
               </font>
            </Label>
            <Button fx:id="loginButton" layoutX="266.0" layoutY="249.0" mnemonicParsing="false" onAction="#loginHandler" text="Login" />
            <Button fx:id="cancel" layoutX="365.0" layoutY="249.0" mnemonicParsing="false" onAction="#close" text="Cancel" />
            <Label fx:id="loginError" layoutX="210.0" layoutY="223.0" prefHeight="17.0" prefWidth="234.0" textFill="RED" />
      </children>
    </AnchorPane>
  </children>
</VBox>
