<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Label?>
<?import javafx.scene.control.Separator?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.text.TextFlow?>
<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8.0.121" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Main.studentMarkListController">

    <VBox layoutY="20" layoutX="20" alignment="CENTER" minWidth="570" maxWidth="570" prefHeight="320" maxHeight="320">

        <Label alignment="CENTER" prefHeight="23.0" text="Your Marks">
            <font>
                <Font name="System Bold" size="17.0" />
            </font>
        </Label>
        <Separator visible="false" />
        <Separator visible="false" />
        <Separator visible="false" />
        <TableView fx:id="stuMarksTable" maxHeight="120.0" prefHeight="320" >
            <columns>
                <TableColumn fx:id="assignNameCol" prefWidth="150.0" text="Assignment" />
                <TableColumn fx:id="attemptOneCol" prefWidth="100.0" text="Attempt 01" />
                <TableColumn fx:id="attemptTwoCol" prefWidth="100.0" text="Attempt 02" />
                <TableColumn fx:id="bestMarkCol" prefWidth="100.0" text="Best Mark" />
            </columns>
        </TableView>
        <Separator visible="false" />
        <Separator visible="false" />
        <TextFlow textAlignment="CENTER">
            <Button fx:id="toSPageButton" onAction="#toStudentPage" text="Back" prefHeight="30" prefWidth="120"/>
        </TextFlow>
    </VBox>
</AnchorPane>
